{
	"name": "@opensession/ui",
	"version": "0.1.0",
	"type": "module",
	"svelte": "./src/index.ts",
	"main": "./src/index.ts",
	"exports": {
		".": {
			"svelte": "./src/index.ts",
			"default": "./src/index.ts"
		},
